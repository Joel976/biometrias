  "identificador_unico": "user_test_${__Random(1,1000)}",&#xd;
  "imagen_base64": "iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAADUlEQVR42mNk+M9QDwADhgGAWjR9awAAAABJRU5ErkJggg==",&#xd;
  "umbral_confianza": 0.7&#xd;
