          <DurationAssertion guiclass="DurationAssertionGui" testclass="DurationAssertion" testname="Max 5s Response Time" enabled="true">
            <stringProp name="DurationAssertion.duration">5000</stringProp>
          </DurationAssertion>
          <hashTree/>
        </hashTree>

